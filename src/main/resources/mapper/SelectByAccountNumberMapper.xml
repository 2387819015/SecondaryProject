<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.secondaryproject.mapper.UserMapper">
    <select id="selectByAccountNumber" parameterType="String" resultType="Integer">
        SELECT count(*) from user_table where account_number = #{accountNumber}
    </select>

</mapper>



